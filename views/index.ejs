    <script>
        $(document).ready(function () {
            var a = $("nav").offset().top;

            $(document).scroll(function () {
                if ($(this).scrollTop() > a) {
                    $('nav').css({ "background-color": "#e4e5f2" });
                } else {
                    $('nav').css({ "background-color": "transparent" });
                }
            });
        });
